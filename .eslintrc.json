{
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "rules": {
      "no-unused-vars":"warn",
      "no-console":"off",
      "semi":["error", "never"],
      "class-methods-use-this":"off",
      "import/prefer-default-export": "off",
      "no-useless-constructor":"off",
      "import/extensions": "off",
      "max-len":"off"
    }
  }